{
  "name": "@ichef/transcharts-graph",
  "version": "0.0.0",
  "description": "Basic reusable graphs",
  "author": "garfieldduck <garfieldduck@gmail.com>",
  "homepage": "https://github.com/iCHEF/transcharts#readme",
  "license": "MIT",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "typings": "lib/esm/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "registry": "https://registry.yarnpkg.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iCHEF/transcharts.git"
  },
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "build": "run-p \"build:*\"",
    "build:esm": "tsc -p ./src",
    "build:cjs": "tsc -p ./src/tsconfig.cjs.json",
    "clean": "rimraf ./dist ./lib ./es5 ./deploy",
    "lint": "tslint --project ./src",
    "type-check": "tsc -p ./src --noEmit",
    "type-check:watch": "tsc -p ./src --noEmit --watch"
  },
  "peerDependencies": {
    "prop-types": "^15.7.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-spring": "^8.0.7",
    "styled-components": "^4.1.3"
  },
  "bugs": {
    "url": "https://github.com/iCHEF/transcharts/issues"
  },
  "dependencies": {
    "@vx/event": "^0.0.182",
    "@vx/legend": "^0.0.183",
    "d3-array": "^2.0.3",
    "d3-scale": "^2.1.2",
    "d3-scale-chromatic": "^1.3.3",
    "deepmerge": "^3.2.0",
    "lodash": "^4.17.11",
    "memoize-one": "^5.0.0",
    "resize-observer-polyfill": "^1.5.1"
  }
}
